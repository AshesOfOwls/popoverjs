{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "airbnb",
    "rules": {
        "indent": ["error", 2],
        'react/forbid-prop-types': 'off',
        'react/no-find-dom-node': 'off',
        'react/jsx-filename-extension': 'off'
    }
}
